;;; -*- Hen -*-
(include "setup-helper")

(verify-extension-name "debug")

(setup-shared-extension-module
 'debug
 (extension-version "0.3.10")
 compile-options: '(-X cock))

(handle-exceptions exn
  (warning "Not generating documentation")
  (load "setup-helper-cock")
  (run-cock -o debug.wiki debug-core.scm))
